<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sara.rehabilitation_tool.Places">

    <Button
        android:id="@+id/salon"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="177dp"
        android:background="@drawable/livingroom"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bathRoom"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="177dp"
        android:background="@drawable/bathroom"
        app:layout_constraintStart_toEndOf="@+id/salon"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bedRoom"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="177dp"
        android:background="@drawable/bed"
        app:layout_constraintStart_toEndOf="@+id/bathRoom"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/superMarket"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/supermarket3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/salon" />

    <Button
        android:id="@+id/university"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/university2"
        app:layout_constraintStart_toEndOf="@+id/superMarket"
        app:layout_constraintTop_toBottomOf="@+id/bathRoom" />

    <Button
        android:id="@+id/school"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/school"
        app:layout_constraintStart_toEndOf="@+id/university"
        app:layout_constraintTop_toBottomOf="@+id/bedRoom" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/salon" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/bathRoom" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/bedRoom" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/superMarket" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="62dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/textView28"
        app:layout_constraintTop_toBottomOf="@+id/university" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/textView29"
        app:layout_constraintTop_toBottomOf="@+id/school" />
</android.support.constraint.ConstraintLayout>
