<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TOOLS">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorPrimaryDark">

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="368dp"
                android:layout_height="203dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:background="@color/colorAccent"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="RtlHardcoded">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="158dp"
                    android:background="@color/colorPrimary" />

                <ImageButton
                    android:id="@+id/playall"
                    style="@style/Widget.AppCompat.Button"
                    android:layout_width="132dp"
                    android:layout_height="30dp"
                    android:layout_marginStart="-200dp"
                    android:layout_marginLeft="-250dp"
                    android:layout_marginTop="170dp"
                    android:background="@drawable/round_button"
                    android:src="@android:drawable/ic_media_play" />
            </LinearLayout>

            <ImageView
                android:id="@+id/dishh"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/dish"
                android:contentDescription="@string/dishh"
                app:layout_constraintStart_toEndOf="@+id/cupp"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <ImageView
                android:id="@+id/cupp"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/mug"
                android:contentDescription="@string/cupp"
                app:layout_constraintStart_toEndOf="@+id/spoonn"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <ImageView
                android:id="@+id/spoonn"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/spoon"
                android:contentDescription="@string/spoonn"
                app:layout_constraintStart_toEndOf="@+id/forkk"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <ImageView
                android:id="@+id/forkk"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="26dp"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/forkkz"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout"
                android:contentDescription="@string/forkk"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/fork2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="26dp"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="10dp"
                android:text="@string/fork2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/forkk"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/spoon2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/spoon2"
                app:layout_constraintStart_toEndOf="@+id/fork2"
                app:layout_constraintTop_toBottomOf="@+id/spoonn"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/cup2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="26dp"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="10dp"
                android:text="@string/cup2"
                app:layout_constraintStart_toEndOf="@+id/spoon2"
                app:layout_constraintTop_toBottomOf="@+id/cupp"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/dish2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/dish2"
                app:layout_constraintStart_toEndOf="@+id/cup2"
                app:layout_constraintTop_toBottomOf="@+id/dishh"
                tools:ignore="RtlHardcoded" />

            <ImageView
                android:id="@+id/knifee"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/knife"
                android:contentDescription="@string/knifee"
                app:layout_constraintStart_toEndOf="@+id/scissorss"
                app:layout_constraintTop_toBottomOf="@+id/dish2" />

            <ImageView
                android:id="@+id/scissorss"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/maas"
                android:contentDescription="@string/scissorss"
                app:layout_constraintStart_toEndOf="@+id/chairr"
                app:layout_constraintTop_toBottomOf="@+id/cup2" />

            <ImageView
                android:id="@+id/chairr"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/chair"
                android:contentDescription="@string/chairr"
                app:layout_constraintStart_toEndOf="@+id/tablee"
                app:layout_constraintTop_toBottomOf="@+id/spoon2" />

            <ImageView
                android:id="@+id/tablee"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/table"
                android:contentDescription="@string/tablee"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/fork2" />

            <TextView
                android:id="@+id/table2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="26dp"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="10dp"
                android:text="@string/table2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tablee"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/chair2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/chair2"
                app:layout_constraintStart_toEndOf="@+id/table2"
                app:layout_constraintTop_toBottomOf="@+id/chairr"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/scissors2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/scussors2"
                app:layout_constraintStart_toEndOf="@+id/chair2"
                app:layout_constraintTop_toBottomOf="@+id/scissorss"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/knife2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/knife2"
                app:layout_constraintStart_toEndOf="@+id/scissors2"
                app:layout_constraintTop_toBottomOf="@+id/knifee"
                tools:ignore="RtlHardcoded" />

            <ImageView
                android:id="@+id/tvv"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/tv"
                android:contentDescription="@string/tvv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/table2" />

            <ImageView
                android:id="@+id/glassess"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/glasses"
                android:contentDescription="@string/glassess"
                app:layout_constraintStart_toEndOf="@+id/tvv"
                app:layout_constraintTop_toBottomOf="@+id/chair2" />

            <ImageView
                android:id="@+id/penn"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/pen"
                android:contentDescription="@string/penn"
                app:layout_constraintStart_toEndOf="@+id/glassess"
                app:layout_constraintTop_toBottomOf="@+id/scissors2" />

            <ImageView
                android:id="@+id/notebookk"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/notebook"
                android:contentDescription="@string/notebookk"
                app:layout_constraintStart_toEndOf="@+id/penn"
                app:layout_constraintTop_toBottomOf="@+id/knife2" />

            <TextView
                android:id="@+id/tv2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="26dp"
                android:layout_marginLeft="26dp"
                android:layout_marginTop="10dp"
                android:text="@string/tv2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tvv"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/glasses2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="111dp"
                android:layout_marginLeft="111dp"
                android:layout_marginTop="10dp"
                android:text="@string/glasses2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/glassess"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/pen2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/pen2"
                app:layout_constraintStart_toEndOf="@+id/glasses2"
                app:layout_constraintTop_toBottomOf="@+id/penn"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/notebook22"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="110dp"
                android:layout_marginLeft="110dp"
                android:layout_marginTop="10dp"
                android:text="@string/notebook22"
                app:layout_constraintStart_toEndOf="@+id/glasses2"
                app:layout_constraintTop_toBottomOf="@+id/notebookk"
                tools:ignore="RtlHardcoded" />

            <ImageView
                android:id="@+id/toyss"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/toys"
                android:contentDescription="@string/toyss"
                app:layout_constraintStart_toEndOf="@+id/watch"
                app:layout_constraintTop_toBottomOf="@+id/glasses2" />

            <ImageView
                android:id="@+id/ball"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/ball"
                android:contentDescription="@string/ball"
                app:layout_constraintStart_toEndOf="@+id/toyss"
                app:layout_constraintTop_toBottomOf="@+id/pen2" />

            <ImageView
                android:id="@+id/computerr"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/pc"
                android:contentDescription="@string/computerr"
                app:layout_constraintStart_toEndOf="@+id/ball"
                app:layout_constraintTop_toBottomOf="@+id/notebook22" />

            <TextView
                android:id="@+id/toys2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/toys2"
                app:layout_constraintStart_toEndOf="@+id/watch2"
                app:layout_constraintTop_toBottomOf="@+id/toyss"
                tools:ignore="UnknownIdInLayout" />

            <TextView
                android:id="@+id/ball2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/ball2"
                app:layout_constraintStart_toEndOf="@+id/toys2"
                app:layout_constraintTop_toBottomOf="@+id/ball"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/computer2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:text="@string/computer2"
                app:layout_constraintStart_toEndOf="@+id/ball2"
                app:layout_constraintTop_toBottomOf="@+id/computerr"
                tools:ignore="RtlHardcoded" />

            <Button
                android:id="@+id/back"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginStart="319dp"
                android:layout_marginLeft="319dp"
                android:layout_marginTop="107dp"
                android:background="@android:drawable/ic_menu_revert"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/computer2"
                tools:ignore="RtlHardcoded" />

            <ImageView
                android:id="@+id/watch"
                android:layout_width="60dp"
                android:layout_height="120dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv2"
                app:srcCompat="@drawable/watch"
                android:contentDescription="@string/watch" />

            <TextView
                android:id="@+id/watchh2"
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:layout_marginStart="26dp"
                android:layout_marginTop="10dp"
                android:text="@string/watchh"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/watch" />
        </android.support.constraint.ConstraintLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>